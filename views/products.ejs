<% include partials/_header %>

<form action="/upload" method="post" enctype="multipart/form-data">
  <input type="file" name="csvfile" />

  <button type="submit" class="btn btn-primary">
    Upload CSV
  </button>
</form>
<a href ="/" class=" btn btn-primary">Fetch Data</a>
      <table class="table table-bordered table-hover" style="width:0">
        <thead class="thead-dark"> 
          <tr >
            <td>SKU</td>
            <td>product_name</td>
            <td>about_product</td>
            <td>product_desc</td>
            <td>product_care</td>
            <td>product_image1</td>
            <td>product_image2</td>
            <td>product_image3</td>
            <td>product_image4</td>
            <td>product_image5</td>
            <td>price</td>
            <td>coupon_code</td>
            <td> product_url</td>
            <td>product_varieties</td>
            <td>color</td>
            <td>size_for_trial</td>
          </tr>
        </thead>
        <tbody>
          <% if (data) { %>
            <% for(var i = 0; i < data.length; i++) { %>
              <tr class>
                <td><%= data[i].SKU%></td>
                <td><%= data[i].product_name %></td>
                <td><%= data[i].about_product %></td>
                <td><%= data[i].product_desc %></td>
                <td><%= data[i].product_care %></td>
                <td><img src="<%= data[i].product_image1%>"height=200 width=200></img> </td>
                <td><img src="<%= data[i].product_image2%>"height=200 width=200></img> </td>
                <td><img src="<%= data[i].product_image3%>"height=200 width=200></img> </td>
                <td><img src="<%= data[i].product_image4%>"height=200 width=200></img> </td>
                <td><img src="<%= data[i].product_image5%>"height=200 width=200></img> </td>
                <td><%= data[i].price%></td>
                <td><%= data[i].coupon_code%></td>
                <td><%= data[i].product_url%></td>
                <td><%= data[i].product_varieties%></td>
                <td><%= data[i].color%></td>
                <td><%= data[i].size_for_trial%></td>
              </tr>
            <% } %>
          <% } %>
        </tbody>
      </table>
      </div>
    

        
   

<% include partials/_footer %>
